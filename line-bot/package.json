  },
  "dependencies": {
    "@line/bot-sdk": "^9.7.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
